<?xml version="1.0" encoding="UTF-8"?>

<project basedir=".." default="build" name="utl.slm">
<echo> utl.slm update...</echo>
<target name="build">
	<!-- utl.slm 파일관리  -->
	<zip destfile="${where}" update="true">
		<fileset dir=".">
			<!-- java -->
			<include name="${path.java}/utl/slm/**/*.java"/>

		
			<!-- message2 -->
			<include name="${path.message}/utl/slm/**/*.properties"/>
		</fileset>
	</zip>
</target>
</project>
